{
	"name": "my-campaigns",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.7",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/jest": "^27.5.2",
		"@types/node": "^17.0.45",
		"@types/react": "^18.3.18",
		"@types/react-dom": "^18.3.5",
		"react": "^19.0.0",
		"react-data-table-component": "^7.6.2",
		"react-dom": "^19.0.0",
		"react-redux": "^8.1.3",
		"react-scripts": "5.0.1",
		"styled-components": "^6.1.15",
		"typescript": "^4.9.5",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"plugin:prettier/recommended"
		],
		"plugins": [
			"react",
			"prettier"
		],
		"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
		},
		"rules": {
			"no-console": "warn",
			"react/prop-types": "off",
			"prettier/prettier": [
				"error"
			]
		}
	},
	"prettier": {
		"tabWidth": 2,
		"semi": false,
		"useTabs": true,
		"singleQuote": false,
		"trailingComma": "none"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/redux-mock-store": "^1.5.0",
		"ajv": "^8.17.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"prettier": "^3.4.2",
		"redux-mock-store": "^1.5.5"
	}
}
